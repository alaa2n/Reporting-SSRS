  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 
  t.Top_Id,
  t.Top_Name,
  c.Crs_Id,
  c.Crs_Name,
  c.Crs_Duration
FROM dbo.Topic t
JOIN dbo.Course c ON c.Top_Id = t.Top_Id
ORDER BY t.Top_Name, c.Crs_Name;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
